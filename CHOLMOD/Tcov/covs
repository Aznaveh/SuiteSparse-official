# usage: covs
echo '================================================================='
foreach file (*.c.?cov)
	echo $file
	grep "#####" $file | grep -v "\<_" | grep -v "return 0;" | \
            grep -v "#####:[ ]*[0-9]*:[{}]"
	echo '================================================================='
	end
